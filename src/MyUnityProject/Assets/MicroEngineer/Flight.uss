    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: rgba(0, 0, 0, 0);
}

.maingui-settings-button:hover {
    background-color: rgba(255, 255, 255, 0.25);
}

.window-root {
    width: 220px;
    background-color: rgb(33, 39, 46);
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 0;
    padding-bottom: 2px;
