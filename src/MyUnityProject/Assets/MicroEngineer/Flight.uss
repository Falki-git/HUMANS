    border-top-width: 1px;
    border-bottom-width: 0;
    flex-shrink: 0;
    height: 23px;
    justify-content: flex-start;
    flex-direction: row;
    align-items: center;
}

.main-gui__header__standalone {
    background-color: rgb(33, 39, 46);
    flex-shrink: 0;
    height: 25px;
